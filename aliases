alias xx="tmux"
alias py="python3"

##listing files
alias l="ls -lGH"
alias ll="ls -1"
alias lt="ls -ltGH"
alias la="ls -laGH"
alias lr="ls -lRGH"

alias dl="youtube-dl -x --audio-format mp3"

## git
alias g="git status -sb"
alias gg="git status"
alias ga="git add"
alias gu="git add -u"
alias gb="git branch"
alias gc="git commit"
alias gco="git checkout"
alias gd="git diff"
alias gl="git log"
alias gll="git log --graph --oneline --all --decorate -n 25"
alias glll="git log --graph --oneline --all --decorate"
alias gf="git ls-tree HEAD --full-tree -r"

alias ..="pwd; cd ..; pwd; ls -lG"

alias ]="less" # because I pipe to less all the time
alias rmd="rm -rf"
alias v="vagrant"

##python
alias py3="python3"
alias ipy="ipython3"
